<h3>Listing photos</h3>

  <ul class="thumbnails">
    <% @photos.each do |photo| %>
     <li class="span3">
     <div class="thumbnail"> 
      <%= photo.name %>
    <%= image_tag(photo.image_url,:size=>'200x240')%><br>
     <%= link_to 'Show', allbum_photo_path(@allbum,photo) %>
    <%= link_to 'Edit', edit_allbum_photo_path(@allbum,photo) %>
    <%= link_to 'Destroy',allbum_photo_path(@allbum,photo), method: :delete, data: { confirm: 'Are you sure?' } %>
    </div>
    </li>
   <% end %>
</ul>  

<br/>

<%= link_to 'Add Photo', new_allbum_photo_path %><br>
<%= link_to 'Back', allbums_path%>